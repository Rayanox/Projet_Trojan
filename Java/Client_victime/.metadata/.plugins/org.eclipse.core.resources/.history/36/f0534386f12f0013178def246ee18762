		this.client = c;
		this.BufferDEcoute = new BufferedReader(new InputStreamReader(this.client.getSocket().getInputStream()));
		
	}
	
	
	public void run() {
		
	}
	
	public void close() {
		
	}
	
	
	
	
	
}
